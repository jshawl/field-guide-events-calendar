{
  "name": "field-guide-events-calendar",
  "version": "1.4.0",
  "description": "events calendar with filtering",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "oxlint",
    "test": "npm run test:js && npm run test:php",
    "test:js": "vitest run --sequence.shuffle",
    "test:coverage": "vitest run --coverage",
    "start": "wp-env start",
    "wp-env": "wp-env",
    "test:php": "wp-env run tests-cli --env-cwd=wp-content/plugins/field-guide-events-calendar vendor/bin/phpunit --coverage-html coverage/php",
    "plugin:check": "wp-env run tests-cli -- wp plugin check field-guide-events-calendar --exclude-directories=tests,coverage,bin --ignore-codes=hidden_files"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prettier": {
    "plugins": [
      "@prettier/plugin-php"
    ]
  },
  "devDependencies": {
    "@prettier/plugin-php": "^0.24.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@wordpress/env": "^10.29.0",
    "jsdom": "^26.1.0",
    "oxlint": "^1.14.0",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4"
  }
}
